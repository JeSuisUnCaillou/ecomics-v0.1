
<%= form_for @frame, :url => frames_path,
                     :html => { :multipart => true, class: "form-inline" } do |f| %>
  <div class="form-group">
    <label class="sr-only" for="frame_file_field">Picture files</label>
    <%= f.file_field :pictures, multiple: true, class:"btn btn-default", id: "frame_file_field" %>
  </div>

  <div class="form-group">
    <%= f.hidden_field :ecomic_id, value: ecomic.id %>
  </div>

  <div class="form-group">
    <%= f.submit "Add Pictures", class: "btn btn-default" %>
  </div>

  <!--div class="field">
    <%#= f.label :name %>
    <%#= f.text_field :name %>
  </div-->
  <!--div class="field">
    <%#= f.label :duration %>
    <%#= f.number_field :duration %> ms
  </div-->
<% end %>


